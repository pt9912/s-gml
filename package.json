{
    "name": "s-gml",
    "version": "1.0.0",
    "description": "TypeScript library for parsing, converting, and validating GML 2.1.2/3.0/3.2",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm run build:esm && pnpm run build:cjs && pnpm run build:types",
        "build:esm": "rollup -c rollup.esm.js",
        "build:cjs": "rollup -c rollup.cjs.js",
        "build:types": "tsc --emitDeclarationOnly",
        "test": "jest",
        "lint": "eslint . --ext .ts",
        "prepublishOnly": "pnpm run build"
    },
    "keywords": [
        "gml",
        "geojson",
        "wfs",
        "spatial",
        "gis"
    ],
    "author": "Dietmar Burkard",
    "license": "MIT",
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=8.0.0"
    },
    "dependencies": {
        "commander": "^11.0.0",
        "xml2js": "^0.6.2",
        "xsd-validator": "^1.1.1"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/jest": "^29.5.4",
        "@types/geojson": "^7946.0.10",
        "@types/xml2js": "^0.4.11",
        "@types/node": "^20.11.0",
        "jest": "^29.6.4",
        "rollup": "^4.9.0",
        "rollup-plugin-dts": "^6.1.0",
        "ts-jest": "^29.1.1",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3"
    }
}
