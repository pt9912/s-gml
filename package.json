{
    "name": "@npm9912/s-gml",
    "version": "1.6.0",
    "description": "TypeScript library for parsing, converting, and validating GML 2.1.2/3.0/3.2",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/pt9912/s-gml"
    },
    "bin": {
        "s-gml": "./dist/cli.js"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm run build:esm && pnpm run build:cjs && pnpm run build:types && pnpm run build:cli",
        "build:esm": "rollup -c rollup.esm.js",
        "build:cjs": "rollup -c rollup.cjs.js",
        "build:types": "tsc --emitDeclarationOnly",
        "build:cli": "rollup -c rollup.cli.js",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "lint": "eslint . --ext .ts",
        "version:bump": "node scripts/bump-version.js",
        "prepublishOnly": "pnpm run build"
    },
    "keywords": [
        "gml",
        "geojson",
        "wfs",
        "wcs",
        "cis",
        "spatial",
        "gis"
    ],
    "author": "Dietmar Burkard",
    "license": "MIT",
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=8.0.0"
    },
    "dependencies": {
        "@mapbox/shp-write": "^0.4.3",
        "commander": "^11.0.0",
        "fast-xml-parser": "^4.5.0",
        "xmllint-wasm": "^5.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/geojson": "^7946.0.10",
        "@types/jest": "^29.5.4",
        "@types/node": "^20.11.0",
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.0.0",
        "jest": "^29.6.4",
        "rollup": "^4.9.0",
        "rollup-plugin-dts": "^6.1.0",
        "ts-jest": "^29.1.1",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3"
    }
}
